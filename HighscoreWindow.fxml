<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" 
            minHeight="-Infinity" minWidth="-Infinity" 
            prefHeight="600.0" prefWidth="400.0" 
            stylesheets="@HighscoreWindow.css"
            fx:controller="pjatu.HighscoreController"
            xmlns="http://javafx.com/javafx/8.0.171" 
            xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox prefHeight="180.0" prefWidth="400.0">
         <children>
            <HBox prefHeight="60.0" prefWidth="400.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" 
                        text="" 
                        textAlignment="CENTER" 
                        underline="true" 
                        fx:id="firstPlace"
                        wrappingWidth="400.0">
                     <font>
                        <Font name="Franklin Gothic Demi" 
                              size="25.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Text>
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </HBox>
            <HBox prefHeight="60.0" prefWidth="400.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" 
                        text="" 
                        textAlignment="CENTER" 
                        underline="true" 
                        fx:id="secondPlace"
                        wrappingWidth="400.0">
                     <font>
                        <Font name="Franklin Gothic Demi" 
                              size="25.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Text>
               </children>
            </HBox>
            <HBox prefHeight="60.0" prefWidth="400.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" 
                        text="" 
                        textAlignment="CENTER" 
                        underline="true" 
                        fx:id="thirdPlace"
                        wrappingWidth="400.0">
                     <font>
                        <Font name="Franklin Gothic Demi" 
                              size="25.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Text>
               </children>
            </HBox>
         </children>
      </VBox>
      <VBox layoutY="180.0" 
            fx:id="places"
            prefHeight="360.0" 
            prefWidth="400.0">
         <children>
            
         </children>
      </VBox>
      <HBox layoutY="540.0" 
            prefHeight="60.0" 
            prefWidth="400.0">
         <children>
            <Button fx:id="exitButton" 
                    mnemonicParsing="false" 
                    prefHeight="60.0" 
                    prefWidth="100.0"
                    onAction="#onExitButton"  
                    text="POWRÓT">
               <font>
                  <Font name="Franklin Gothic Heavy" 
                        size="15.0" />
               </font>
            </Button>
            <Button fx:id="easyButton" 
                    mnemonicParsing="false" 
                    prefHeight="60.0" prefWidth="100.0" 
                    styleClass="notClicked"
                    onAction="#onEasyButton" 
                    text="ŁATWY">
               <font>
                  <Font name="Franklin Gothic Heavy" 
                        size="15.0" />
               </font>
            </Button>
            <Button fx:id="mediumButton" 
                    mnemonicParsing="false" 
                    prefHeight="60.0" prefWidth="100.0"
                    styleClass="notClicked" 
                    onAction="#onMediumButton" 
                    text="ŚREDNI">
               <font>
                  <Font name="Franklin Gothic Heavy" 
                        size="15.0" />
               </font>
            </Button>
            <Button fx:id="hardButton" 
                    mnemonicParsing="false" 
                    prefHeight="60.0" prefWidth="100.0"
                    styleClass="notClicked" 
                    onAction="#onHardButton" 
                    text="TRUDNY">
               <font>
                  <Font name="Franklin Gothic Heavy" 
                        size="15.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
